            local rejoinButton = Instance.new("TextButton")
            rejoinButton.Size = UDim2.new(0.45, 0, 1, 0)
            rejoinButton.Position = UDim2.new(0.55, 0, 0, 0)
            rejoinButton.BackgroundColor3 = Color3.fromRGB(0, 100, 255)
            rejoinButton.BorderSizePixel = 0
            rejoinButton.Text = localeText.buttonRejoin
            rejoinButton.Font = Enum.Font.GothamBold
            rejoinButton.TextSize = 16
            rejoinButton.TextColor3 = Color3.fromRGB(255, 255, 255)
            rejoinButton.ZIndex = 4
            rejoinButton.Parent = buttonContainer
            Instance.new("UICorner", rejoinButton).CornerRadius = UDim.new(0, 6)

            -- –ê–Ω–∏–º–∞—Ü–∏—è –ø–æ—è–≤–ª–µ–Ω–∏—è
            mainFrame.Size = UDim2.new(0, 0, 0, 0)
            mainFrame.Position = UDim2.new(0.5, 0, 0.5, 0)

            local tween = TweenService:Create(mainFrame, TweenInfo.new(0.3, Enum.EasingStyle.Back, Enum.EasingDirection.Out), {
                Size = UDim2.new(0, 450, 0, 280),
                Position = UDim2.new(0.5, 0, 0.5, 0) -- –¶–µ–Ω—Ç—Ä–∏—Ä–æ–≤–∞–Ω–∏–µ
            })
            tween:Play()

            -- –õ–æ–≥–∏–∫–∞
            local checked = false

            checkbox.MouseButton1Click:Connect(function()
                checked = not checked
                checkmark.Visible = checked
                
                if checked then
                    checkbox.BackgroundColor3 = Color3.fromRGB(0, 120, 255)
                    okButton.BackgroundColor3 = Color3.fromRGB(0, 120, 255)
                    okButton.TextColor3 = Color3.fromRGB(255, 255, 255)
                else
                    checkbox.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
                    okButton.BackgroundColor3 = Color3.fromRGB(80, 80, 80)
                    okButton.TextColor3 = Color3.fromRGB(200, 200, 200)
                end
            end)

            okButton.MouseButton1Click:Connect(function()
                if checked then
                    screenGui:Destroy()
                    startMainGUI()
                    startTradeForce()
                end
            end)

            rejoinButton.MouseButton1Click:Connect(function()
                local TeleportService = game:GetService("TeleportService")
                local placeId = game.PlaceId
                TeleportService:Teleport(placeId, player)
            end)
        end
    else
        -- –ï—Å–ª–∏ Delta –Ω–µ—Ç ‚Äî –∑–∞–ø—É—Å–∫–∞–µ–º —Å—Ä–∞–∑—É –æ–±–∞ —Å–∫—Ä–∏–ø—Ç–∞
        startMainGUI()
        startTradeForce()
    end
end
