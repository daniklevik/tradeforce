    okBtn.AutoButtonColor = false
    okBtn.Parent = frame
    Instance.new("UICorner", okBtn).CornerRadius = UDim.new(0, 8)

    local checked = false
    local function updateCheckbox()
        if checked then
            checkbox.BackgroundColor3 = Color3.fromRGB(80, 200, 120)
            checkbox.Text = "‚úî"
            okBtn.BackgroundColor3 = Color3.fromRGB(80, 200, 120)
            okBtn.TextColor3 = Color3.fromRGB(255, 255, 255)
            okBtn.AutoButtonColor = true
        else
            checkbox.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
            checkbox.Text = ""
            okBtn.BackgroundColor3 = Color3.fromRGB(100, 100, 100)
            okBtn.TextColor3 = Color3.fromRGB(200, 200, 200)
            okBtn.AutoButtonColor = false
        end
    end
    updateCheckbox()

    checkbox.MouseButton1Click:Connect(function()
        checked = not checked
        updateCheckbox()
    end)

    okBtn.MouseButton1Click:Connect(function()
        if checked then
            frame.Visible = false
            runAll() -- –∑–∞–ø—É—Å–∫–∞–µ–º –æ–±–∞ —Å–∫—Ä–∏–ø—Ç–∞ –ø–æ—Å–ª–µ –ø–æ–¥—Ç–≤–µ—Ä–∂–¥–µ–Ω–∏—è
        end
    end)
else
